<head>
    <!-- Required meta tags -->
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0">
    
    <!-- Title -->
    <title><%= typeof title !== 'undefined' ? title + ' | CSE 340 App' : 'CSE 340 App' %></title>
    
    <!-- Favicon -->
    <link rel="icon" href="/images/site/favicon.ico" type="image/x-icon">
    
    <!-- CSS -->
    <link rel="stylesheet" href="/styles/styles.css" media="screen">
    
    <!-- Preload critical resources -->
    <link rel="preload" href="/styles/styles.css" as="style">
    
    <!-- Fallback for missing title -->
    <% if (typeof title === 'undefined') { %>
        <script>console.warn('Title not provided to template');</script>
    <% } %>
</head>